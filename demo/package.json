{
  "name": "ng2-demo",
  "description": "A demo site for @rinminase/ng-charts",
  "homepage": "https://ng-charts.netlify.com/",
  "version": "0.1.0",
  "license": "MIT",
  "author": "Rin Minase",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RinMinase/ng-charts.git"
  },
  "scripts": {
    "start": "ng serve",
    "build": "ng build --prod && npm run redirects",
    "deploy": "ng build --prod --no-progress && npm run redirects",
    "redirects": "echo /* /index.html 200> ./dist/_redirects"
  },
  "dependencies": {
    "@angular/animations": "^9.0.5",
    "@angular/common": "^9.0.5",
    "@angular/core": "^9.0.5",
    "@angular/material": "^9.1.1",
    "@angular/platform-browser-dynamic": "^9.0.5",
    "@rinminase/ng-charts": "^1.3.2",
    "material-icons": "^0.3.1",
    "rxjs": "^6.5.4",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.900.5",
    "@angular/cdk": "^9.1.1",
    "@angular/cli": "^9.0.5",
    "@angular/compiler": "^9.0.5",
    "@angular/compiler-cli": "^9.0.5",
    "@angular/forms": "^9.0.5",
    "@angular/platform-browser": "^9.0.5",
    "netlify-cli": "^2.37.0",
    "tslib": "^1.11.1",
    "typescript": "~3.7.5"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari versions",
    "last 2 iOS versions"
  ],
  "prettier": {
    "printWidth": 80,
    "bracketSpacing": true,
    "trailingComma": "all",
    "arrowParens": "always",
    "endOfLine": "lf"
  }
}
